<Window x:Class="Calculadora.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculadora"
        xmlns:y="clr-namespace:Calculadora.View"
        mc:Ignorable="d"
        Title="Calculator" Height="500" Width="400" Topmost="True">
    <Grid>
        <y:ucLogin x:Name="ucLoginfrm"  Sucess="UcLoginfrm_sucess" Fail="UcLoginfrm_Fail"/>
        <y:ucCalc x:Name="ucCalc" Visibility="Hidden"></y:ucCalc>
    </Grid>
</Window>
